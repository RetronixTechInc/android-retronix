<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Choices for screen output orientation.-->
    <string-array name="orientation_entries">
        <item>0 degrees</item>
        <item>90 degrees</item>
        <item>180 degrees</item>
        <item>270 degrees</item>
        <!--<item>Accelerometer</item>-->
    </string-array>
    <string-array name="orientation_value" translatable="false">
        <item>0</item>
        <item>90</item>
        <item>180</item>
        <item>270</item>
        <!--<item>360</item>-->
        </string-array>
    
    <!-- Choices for screen output.-->
    <string-array name="screenport_entries">
        <item>VGA or LVDS</item>
        <item>HDMI</item>
        <!--<item>DUAL</item>-->
        </string-array>
    <string-array name="screenport_value" translatable="false">
        <item>0</item>
        <item>1</item>
        <!--<item>2</item>-->
        </string-array>
    
    <!-- Choices for screen output resolution.-->
    <string-array name="resolution_entries">
        <!-- <item>auto</item>-->
        <item>800x600</item>
        <item>1024x768</item>
        <!--<item>1280x800</item>-->
        <item>1366x768</item>
        <item>1920x1080</item>
        </string-array>
    <string-array name="resolution_value" translatable="false">
        <!-- <item>0</item>-->
        <item>1</item>
        <item>2</item>
        <!--<item>3</item>-->
        <item>4</item>
        <item>5</item>
        
    </string-array>
  
    <!-- Choices for poower off.-->
    <string-array name="power_entries">
        <item>On</item>
        <item>Off</item>
    </string-array>
    <string-array name="power_value" translatable="false">
        <item>1</item>
        <item>0</item>
    </string-array>
    
</resources>
