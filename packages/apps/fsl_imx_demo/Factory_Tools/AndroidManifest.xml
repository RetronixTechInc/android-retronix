    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".TestForFactory"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
                
        <!--<category android:name="android.intent.category.LAUNCHER" />  -->    	

        <receiver android:name=".MPToolReceiver">
			<intent-filter>
				<action android:name="android.intent.MPToolaction.send"/>
			</intent-filter>
		</receiver> 
        
        <activity android:name=".TestVideo">
        	<intent-filter>
                <action android:name="android.intent.action.Video" />
            </intent-filter>/
        </activity>
        
        <activity android:name=".Writetommc"></activity>
        <activity android:name=".CheckResult"></activity>
        
        <activity android:name=".TestColor">
        	<intent-filter>
                <action android:name="android.intent.action.Color" />
            </intent-filter>/
        </activity>
        
        <activity android:name=".TestKeypad">
        	<intent-filter>
                <action android:name="android.intent.action.Keypad" />
            </intent-filter>/
        </activity>
        
        <activity android:name=".TestFinish">
        	<intent-filter>
                <action android:name="android.intent.action.Finish" />
            </intent-filter>/
        </activity>
        
    </application>
    
